{
    "version": 2,
    "name": "disqusjs-proxy",
    // "alias": [
    //     "disqus.mbioq.com"
    // ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "cache-control": "public, max-age=2171001711771"
            },
            "dest": "https://disqus.com/api/$1"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    }
}